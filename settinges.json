{
    "editor.fontFamily": "Operator Mono,'Fira Code'",
    "editor.fontLigatures": true,
    "editor.renderWhitespace": "none",
    "git.autofetch": true,
    "[php]": {
        "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
    },
    "explorer.confirmDelete": false,
    "php-cs-fixer.executablePath": "${extensionPath}/php-cs-fixer.phar",
    "workbench.colorCustomizations": {

        "[Material Theme Darker]": {}
    },
    "workbench.editor.untitled.hint": "hidden",
    "security.workspace.trust.untrustedFiles": "newWindow",
    "liveServer.settings.donotShowInfoMsg": true,
    "python.defaultInterpreterPath": "/System/Library/Frameworks/Python.framework/Versions/2.7/Resources/Python.app/Contents/MacOS/Python",
    "gitlens.advanced.messages": {
        "suppressGitMissingWarning": true
    },
    "editor.accessibilityPageSize": 12,
    "editor.fontSize": 15,
    "terminal.integrated.fontFamily": "FuraMono Nerd Font",
    "bracketPairColorizer.depreciation-notice": false,
    "[vue]": {
        "editor.defaultFormatter": "octref.vetur"
    },
    "emmet.includeLanguages": {
        "vue": "html",
        "vue-html": "html"
    },
    "vetur.format.defaultFormatter.html": "js-beautify-html",
    "explorer.confirmDragAndDrop": false,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "wrap.with.unless": {
        "label": "@unless",
        "description": "@unless...@endunless",
        "disabled": false,
        "snippet": "@unless ($1) \n\t$TM_SELECTED_TEXT\n@endunless$0"
    },
    "[dart]": {
        "editor.formatOnSave":true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false
    },
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": false,
    "editor.renderControlCharacters": false,
    "workbench.editor.splitInGroupLayout": "vertical",
    "workbench.colorTheme": "Community Material Theme Darker High Contrast",
    "workbench.activityBar.visible": false,
    "editor.fontWeight": "normal",
    "workbench.iconTheme": "material-icon-theme",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
          {
            "scope": "comment",
            "settings": {
              "fontStyle": "italic"
            }
          }
        ]
    },
    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorBlinking": "expand",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false,
    "workbench.statusBar.visible": false,
    "editor.codeActionsOnSave": {
    }
}
